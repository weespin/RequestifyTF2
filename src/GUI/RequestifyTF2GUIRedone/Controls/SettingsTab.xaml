<UserControl x:Class="RequestifyTF2GUIRedone.Controls.SettingsTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RequestifyTF2GUIRedone.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*" />
            <RowDefinition Height="92*" />
        </Grid.RowDefinitions>
        <Button BorderBrush="{x:Null}" Content="{DynamicResource s_ManualBrowser}" HorizontalAlignment="Left"
                            Margin="387.041,120.998,0,0" Grid.Row="1"
                            VerticalAlignment="Top" Width="420.822" Click="Button_Click_1" />
        <Label x:Name="GamePath" Content="" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="1"
                           VerticalAlignment="Top" Width="543" Height="37" />
        <Label Content="{DynamicResource s_Statistics}" HorizontalAlignment="Left" Margin="10,151,0,0" Grid.Row="1"
                           VerticalAlignment="Top" Width="372" />
        <Label Content="0" x:Name="LinesParsed" Margin="10,182,286,0" Grid.Row="1"
                           VerticalAlignment="Top" RenderTransformOrigin="1.889,0.452" />
        <Label Content="0" x:Name="CommandsParsed" HorizontalAlignment="Left" Margin="10,213,0,0"
                           Grid.Row="1" VerticalAlignment="Top" Width="267" />
        <Label Content="0" x:Name="GameKills" HorizontalAlignment="Left" Margin="10,244,0,0" Grid.Row="1"
                           VerticalAlignment="Top" Width="267" />
        <Label Content="0" x:Name="YourKills" HorizontalAlignment="Left" Margin="10,275,0,0" Grid.Row="1"
                           VerticalAlignment="Top" Width="267" />
        <Label Content="0" x:Name="TotalKills" HorizontalAlignment="Left" Margin="10,306,0,0" Grid.Row="1"
                           VerticalAlignment="Top" Width="267" />
        <Label Content="0" x:Name="YourCritKills" HorizontalAlignment="Left" Margin="10,337,0,0"
                           Grid.Row="1" VerticalAlignment="Top" Width="267" />
        <Label Content="0" x:Name="GameCritKills" HorizontalAlignment="Left" Margin="10,368,0,0"
                           Grid.Row="1" VerticalAlignment="Top" Width="267" />
        <Label Content="0" x:Name="TotalCritKills" HorizontalAlignment="Left" Margin="387.041,367.999,0,0"
                           Grid.Row="1" VerticalAlignment="Top" Width="298" />
        <Label Content="0" x:Name="YourDeaths" HorizontalAlignment="Left" Margin="387.041,181.999,0,0" Grid.Row="1"
                           VerticalAlignment="Top" Width="298" />
        <Label Content="0" x:Name="GameDeaths" HorizontalAlignment="Left" Margin="387.041,212.719,0,0" Grid.Row="1"
                           VerticalAlignment="Top" RenderTransformOrigin="0,0.548" Width="298" />
        <Label Content="0" x:Name="TotalDeaths" HorizontalAlignment="Left" Margin="387.041,243.719,0,0" Grid.Row="1"
                           VerticalAlignment="Top" Width="298" />
        <Label Content="0" x:Name="YourSuicides" HorizontalAlignment="Left" Margin="387.041,274.998,0,0" Grid.Row="1"
                           VerticalAlignment="Top" Width="298" />
        <Label Content="0" x:Name="GameSuicides" HorizontalAlignment="Left" Margin="387.041,305.999,0,0" Grid.Row="1"
                           VerticalAlignment="Top" RenderTransformOrigin="1.111,-1.677" Width="298" />
        <Label Content="0" x:Name="TotalSuicides" HorizontalAlignment="Left" Margin="387.041,336.999,0,0"
                           Grid.Row="1" VerticalAlignment="Top" Width="298" />
        <Button BorderBrush="{x:Null}" Click="GameBrowser_click" Content="{DynamicResource s_GameBrowser}"
                            HorizontalAlignment="Left" Margin="10,120.999,0,0" Grid.Row="1" VerticalAlignment="Top"
                            Width="372.041" />
        <Label Content="{DynamicResource s_TF_Only}" Margin="576,394.997,0,0" Grid.Row="1" Width="241.863" Foreground="#99000000" RenderTransformOrigin="0.74,3.871" HorizontalAlignment="Center" VerticalAlignment="Top" />
        <Menu HorizontalAlignment="Left" x:Name="Language"  Height="20" Margin="10,96,0,0" Grid.Row="1" VerticalAlignment="Top" Width="372.041" Background="{x:Null}">
            <MenuItem x:Name="menuLanguage" Header="{DynamicResource s_UI_Lang}"  Height="20" Width="372.041" RenderTransformOrigin="0.5,0.5" />
        </Menu>
        <Menu HorizontalAlignment="Left" x:Name="CoreLanguage"  Height="20" Margin="387.041,96,0,0" Grid.Row="1" VerticalAlignment="Top" Width="420.822" Background="{x:Null}">
            <MenuItem  x:Name="menuCoreLanguage" Header="{DynamicResource s_Core_Lang}" Height="20" Width="420.822" />
        </Menu>

    </Grid>
</UserControl>
